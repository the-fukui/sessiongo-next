{
  "name": "functions",
  "packageManager": "yarn@3.0.2",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc -w",
    "serve": "npm run build:watch && firebase emulators:start --only functions",
